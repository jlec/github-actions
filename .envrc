# shellcheck shell=bash
# shellcheck disable=SC1083,SC1090

set -o errexit

source_env_if_exists ../.envrc

export GITHUB_OWNER=jlec
export GITHUB_USER=jlec

export TRIVY_CONFIG=${PWD}/.trivy.yml
export TRIVY_IGNOREFILE=${PWD}/.trivyignore.yml

__RC=(
)
for i in "${__RC[@]}"; do
	if [[ -e "${HOME}/.${i}.rc" ]]; then
		. "${HOME}/.${i}.rc"
	fi
done
unset __RC i

layout uv
